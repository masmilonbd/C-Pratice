using System;

namespace program
{
    class Program
    {
        static void Main()
        {
            var user = new User();
            if (user.status == User.RegistrationStatus.Active && user.ustype == User.UserType.Admin)
            {
                Console.WriteLine("Login Successfull");
            }

            Console.ReadKey();

        }

    }


    public class User
    {
        public string Name { get; set; }
        public int Age { get; set; }
        public RegistrationStatus status { get; set; }
        public UserType ustype { get; set; }
   

    public enum RegistrationStatus
    {
        Active,
        Inactive,
        Blocked
    }

    public enum UserType
    {
        Admin,
        Support_Executive,
        Member
    }

}
